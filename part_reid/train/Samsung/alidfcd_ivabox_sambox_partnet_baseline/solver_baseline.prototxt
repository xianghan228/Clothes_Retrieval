net: "train_baseline.prototxt"
# The learning rate policy
base_lr: 0.01 
lr_policy: "step"
stepsize:20000
gamma: 0.2
# other hyper parameters
momentum: 0.9
weight_decay: 0.0002
# Display every 20 iterations
#display: 1
display: 20
average_loss: 20
# The maximum number of iterations
max_iter: 100000
# snapshot intermediate results
snapshot: 5000
snapshot_prefix: "snapshot_baseline/model"

